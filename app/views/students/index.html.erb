<h1>Listing all the students</h1>

<table>
    <thead>
        <th>First name</th>
        <th>Last name</th>
        <th>Email address</th>
        <th>Action</th>
    </thead>
    <tbody>
    <% @students.each do |student| %>
        <tr>
            <td><%= student.first_name %></td>
            <td><%= student.last_name %></td>
            <td><%= student.email %></td>
            <td><%= link_to "View" student %></td>
        </tr>
    <% end %>
    </tbody>
</table>